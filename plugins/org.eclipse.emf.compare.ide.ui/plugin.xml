<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.compare.structureMergeViewers">
      <viewer
            class="org.eclipse.emf.compare.internal.ide.ui.structuremergeviewer.EMFCompareStructureMergeViewerCreator"
            extensions="xmi"
            id="org.eclipse.emf.compare.internal.ide.ui.structureMergeViewer"
            label="EMFCompare Structure Merge Viewer">
      </viewer>
      <contentTypeBinding
            contentTypeId="org.eclipse.emf.ecore.xmi"
            structureMergeViewerId="org.eclipse.emf.compare.internal.ide.ui.structureMergeViewer">
      </contentTypeBinding>
   </extension>
   <extension
         point="org.eclipse.compare.contentMergeViewers">
      <viewer
            class="org.eclipse.emf.compare.internal.ide.ui.contentmergeviewer.EObjectContentMergeViewerCreator"
            extensions="eobject"
            id="org.eclipse.emf.compare.internal.ide.ui.EObjectContentMergeViewer"
            label="EMFCompare Content Merge Viewer">
      </viewer>
      <contentTypeBinding
            contentMergeViewerId="org.eclipse.emf.compare.internal.ide.ui.EObjectContentMergeViewer"
            contentTypeId="org.eclipse.emf.ecore.xmi">
      </contentTypeBinding>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.emf.compare.ide.logical.EMFModelProvider"
            class="org.eclipse.emf.compare.ide.ui.logical.adapter.EMFCompareAdapterFactory">
         <adapter
               type="org.eclipse.team.ui.mapping.ISynchronizationCompareAdapter">
         </adapter>
      </factory>
   </extension>
</plugin>