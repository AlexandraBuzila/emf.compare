<plugin>
   <extension-point id="engine" name="engine" schema="schema/engine.exsd"/>
   <extension-point id="mergeFactory" name="mergeFactory" schema="schema/mergeFactory.exsd"/>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.eclipse.org/emf/compare/diff/1.0" 
       class = "org.eclipse.emf.compare.diff.DiffPackage"
       genModel = "model/diff.genmodel" /> 
  </extension>
  <extension
         point="org.eclipse.emf.compare.diff.engine">
      <engine
            engineClass="org.eclipse.emf.compare.diff.generic.DiffMaker"
            priority="low"/>
   </extension>
   <extension
         point="org.eclipse.emf.compare.diff.mergeFactory">
      <factory
            class="org.eclipse.emf.compare.diff.generic.merge.impl.DefaultMergeFactory"
            priority="low"/>
   </extension>

</plugin>

